</ol>\r\n                  <!-- 选项为填空题 -->\r\n                  <ul v-else-if=\"props.row.type == 3\">\r\n                    <li\r\n                      v-for=\"(item, index) of filterOptions(props.row.options)\"\r\n                      :key=\"index\"\r\n                    >\r\n                      <span style=\"font-size:14px;color:#b4b6bd\"\r\n                        >填空 {{ index + 1 }}：</span\r\n                      >\r\n                      <span\r\n                        style=\"font-size:15px\"\r\n                        v-for=\"(i, index) of item\"\r\n                        :key=\"index\"\r\n                      >\r\n                        {{ i.text }}\r\n                        <span style=\"color:red;font-size:2px\">/</span>\r\n                      </span>\r\n                    </li>\r\n                  </ul>\r\n                  <!-- 选项为问答题 -->\r\n                  <ul v-else-if=\"props.row.type == 4\">\r\n                    <li\r\n                      v-for=\"(item, index) of filterOptions(props.row.options)\"\r\n                      :key=\"index\"\r\n                    >\r\n                      <span style=\"font-size:14px;color:#b4b6bd\"\r\n                        >关键词{{ index + 1 }}：</span\r\n                      >{{ item.text }}\r\n                    </li>\r\n                  </ul>\r\n                </el-form-item>\r\n                <el-form-item label=\"答案\">\r\n                  <span v-if=\"props.row.type == 1\">\r\n                    {{abc[props.row.answer-1]}}\r\n                  </span>\r\n                  <span v-else-if=\"props.row.type == 2\">\r\n                    <span v-if=\"props.row.answer == 1\">正确</span>\r\n                    <span v-else>错误</span>\r\n                  </span>\r\n                  <span v-else-if=\"props.row.type == 3\">- -</span>\r\n                  <span v-else-if=\"props.row.type == 4\">{{ props.row.answer }}</span>\r\n                </el-form-item>\r\n                <el-form-item label=\"难度\">\r\n                  <span v-if=\"props.row.degree == 1\">简单</span>\r\n                  <span v-else-if=\"props.row.degree == 2\">普通</span>\r\n                  <span v-else-if=\"props.row.degree == 3\">困难</span>\r\n                  <span v-else>None</span>\r\n                </el-form-item>\r\n                <el-form-item v-if=\"props.row.type == 4\" label=\"解析\">\r\n                  <span>- -</span>\r\n                </el-form-item>\r\n                <el-form-item v-else label=\"解析\">\r\n                  <span>{{ props.row.analysis }}</span>\r\n                </el-form-item>\r\n              </el-form>\r\n            </template>\r\n          </el-table-column>\r\n          <!-- 图标 -->\r\n          <el-table-column label=\"\" width=\"25\">\r\n            <template>\r\n              <i class=\"el-icon-tickets\" style=\"color:#1a8dff\"></i>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column label=\"试题内容\" prop=\"subject_title\" min-width=\"250\" max-width=\"350\">\r\n          </el-table-column>\r\n          <el-table-column label=\"题型\" prop=\"type\" width=\"120\">\r\n            <template slot-scope=\"scope\">\r\n              <span>{{ queType[scope.row.type] }}</span>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column label=\"分类\" width=\"180\">\r\n            <template>\r\n              <!-- 自定义拼接分类 -->\r\n              <span>{{nClassJoin}}</span>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column label=\"难度\" prop=\"degree\" width=\"120\">\r\n            <template slot-scope=\"scope\">\r\n              <span>{{ quedegree[scope.row.degree] }}</span>\r\n            </template>\r\n          </el-table-column>\r\n          <!-- <el-table-column prop=\"address\" label=\"地址\" width=\"300\">\r\n          </el-table-column> -->\r\n          <el-table-column label=\"创建时间\" prop=\"create_time\" width=\"150\">\r\n            <template slot-scope=\"scope\">\r\n              {{ scope.row.create_time | dataTZ() }}\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column fixed=\"right\" label=\"操作\" width=\"100\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button\r\n                type=\"text\"\r\n                size=\"small\"\r\n                @click=\"delQuest(scope.row.id, scope.$index)\"\r\n                >删除</el-button\r\n              >\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n\r\n        <!-- 分页 -->\r\n        <el-pagination\r\n          v-if=\"treeNodeValue\"\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"page_size.page\"\r\n          :page-sizes=\"[5, 10, 20, 30]\"\r\n          :page-size=\"page_size.size\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\"\r\n        >\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {gettitle1} from 'network/typetitle'\r\nexport default {\r\n  data() {\r\n    return {\r\n      // 左分类数据\r\n      classifyData: [],\r\n      // 左分类tree树形控件配置\r\n      defaultProps: {\r\n        children: \"children\",\r\n        label: \"packName\",\r\n      },\r\n      // 点击分类后的表格信息\r\n      tableData: [],\r\n      classOneData: {},\r\n      // 分类到那一层\r\n      className: \"\",\r\n\r\n      // 表格显示或隐藏\r\n      tableFloat: true,\r\n\r\n      // 分页\r\n      page_size: {\r\n        page: 1,\r\n        size: 5,\r\n      },\r\n      // 当前页总条数\r\n      total: 1,\r\n\r\n      // 控制树形分页是否显示\r\n      treeNodeValue: false,\r\n\r\n      // 试题类修type\r\n      queType: {\r\n        1: \"单选题\",\r\n        2: \"判断题\",\r\n        3: \"填空题\",\r\n        4: \"简答题\",\r\n      },\r\n      // 试题难度degree\r\n      quedegree: {\r\n        1: \"简单\",\r\n        2: \"普通\",\r\n        3: \"困难\",\r\n      },\r\n      // 用于拼接所属分类\r\n      nClassJoin:''\r\n    };\r\n  },\r\n  methods: {\r\n    // 左树形分类点击\r\n    async handleNodeClick(data) {\r\n      this.classOneData = data;\r\n\r\n      if (data.level == 2) {\r\n        // 获取到分类拼接好 // 拼接分类到那一层\r\n        let msg1 = await this.http.get(\"/questionclassify/\"+data.pid);\r\n        let msg0 = await this.http.get(\"/questionclassify/\"+msg1.data.data[0].pid);\r\n        console.log(msg1,'msg1');\r\n        console.log(msg0,'msg0');\r\n        this.nClassJoin = `${msg0.data.data[0].packName} / ${msg1.data.data[0].packName} / ${data.packName}`\r\n\r\n        this.tableFloat = false; // 隐藏默认的表格数据\r\n        // 获取试题\r\n        this.handleSizeChange(5);\r\n        this.treeNodeValue = true;\r\n      } else if (data.level == 0) {\r\n        let msg = await this.http.get(\"/questionclassify/\", {\r\n          params: { level: 2 },\r\n        });\r\n        console.log(msg,'msg');\r\n        this.tableData = msg.data.list;\r\n        // 隐藏分页\r\n        this.treeNodeValue = false;\r\n        // 显示默认的表格数据\r\n        this.tableFloat = true;\r\n      } else if ((data.level == 1) & !(\"children\" in data)) {\r\n        this.$message({\r\n          message: \"该二级分类下没有试题哦！\",\r\n          type: \"warning\",\r\n        });\r\n      }\r\n    },\r\n    // 过滤options数据\r\n    filterOptions(v) {\r\n      return JSON.parse(v);\r\n    },\r\n\r\n    // 获取二级item信息\r\n    getItemer(item) {\r\n      if ((\"children\" in item) & (item.children instanceof Array)) {\r\n        if (item.children.length != 0) {\r\n          this.tableData = item.children;\r\n        }\r\n      }\r\n      if (item.level == 2) {\r\n        // 调用获取该分类下的试题\r\n        this.handleNodeClick(item);\r\n        // 显示分页\r\n        this.treeNodeValue = true;\r\n      }\r\n    },\r\n    // 获取树形分类\r\n    async getQuestionclassify() {\r\n      let msg = await this.http.get(\"/questionclassify/\", {\r\n        params: { level: 2 },\r\n      });\r\n      console.log(msg);\r\n      this.classifyData = msg.data.list;\r\n      this.tableData = msg.data.list;\r\n      this.total = msg.data.total;\r\n    },\r\n\r\n    // 获取分类下的试题\r\n    async getQuestions(data) {\r\n      let msg = await this.http.get(\"/questionsLibrary/\", {\r\n        params: { category_id: data.id, ...this.page_size },\r\n      });\r\n      console.log(msg,'msg');\r\n      this.tableData = msg.data.data;\r\n      this.total = msg.data.total;\r\n    },\r\n\r\n    // 删除单个试题\r\n    delQuest(id, index) {\r\n      this.$confirm(\"此操作将永久删除该试题, 是否继续?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then(async () => {\r\n          let msg = await this.http.delete(\"questionsLibrary/\", {\r\n            data: { id: id },\r\n          });\r\n          if (msg.data.state == 200) {\r\n            // 删除当前这条数据\r\n            this.tableData.splice(index, 1);\r\n            this.$message({\r\n              type: \"success\",\r\n              message: \"删除成功!\",\r\n            });\r\n          } else {\r\n            this.$message({\r\n              type: \"error\",\r\n              message: \"删除失败!\" + msg.data.message,\r\n            });\r\n          }\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消删除\",\r\n          });\r\n        });\r\n    },\r\n\r\n    // `每页 ${val} 条`\r\n    handleSizeChange(val) {\r\n      this.page_size.page = 1;\r\n      this.page_size.size = val;\r\n      this.getQuestions(this.classOneData); // 获取一下当前分类下试题\r\n    },\r\n    // `当前页: ${val}`\r\n    handleCurrentChange(val) {\r\n      this.page_size.page = val;\r\n      this.getQuestions(this.classOneData); // 获取一下当前分类下试题\r\n    },\r\n  },\r\n  computed: {\r\n    // 试卷abcd\r\n    abc() {\r\n      let abc = [];\r\n      for (let i = 0; i <= 26; i++) {\r\n        abc.push(String.fromCodePoint(65 + i));\r\n      }\r\n      return abc;\r\n    },\r\n  },\r\n  components: {},\r\n  async created() {\r\n    this.getQuestionclassify(); // 获取一下树形分类\r\n  },\r\n  mounted() {},\r\n  filters: {\r\n//     dataTZ(v) {\r\n//       const moment = require(\"moment\");\r\n//       const timeStr = moment(v).format(\"Y-MM-DD HH:mm:ss\");\r\n//       return timeStr;\r\n//     },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.questions {\r\n  margin-top: -20px;\r\n  display: flex;\r\n  height: 100%;\r\n}\r\n\r\n.questions .content-left {\r\n    width: 20%;\r\n    height: 100%;\r\n  }\r\n\r\n.questions .content-left .title {\r\n      line-height: 68px;\r\n      font-size: 20px;\r\n      font-weight: 500;\r\n    }\r\n.questions .content-left .all-class {\r\n      position: absolute;\r\n      top: 0;\r\n      left: 0;\r\n      width: 100%;\r\n      height: 50px;\r\n      line-height: 50px;\r\n      border-bottom: 1px solid #ebeef6;\r\n      font-size: 14px;\r\n      font-weight: 500;\r\n      color: #6d717c;\r\n      padding-left: 15px;\r\n      background: #fff;\r\n    }\r\n.questions .content-left .el-tree {\r\n      padding: 0 10px;\r\n      height: 100%;\r\n      overflow: auto;\r\n    }\r\n.questions .content-left .el-tree .el-tree-node,.el-tree-node__content {\r\n        height: auto;\r\n        line-height: 45px;\r\n      }\r\n\r\n\r\n.questions  .content-right {\r\n    max-width: 1280px;\r\n    min-width: 1080px;\r\n    min-height: 90%;\r\n  }\r\n\r\n .questions  .content-right .detail-list {\r\n      padding-top: 0;\r\n      min-height: 100%;\r\n      \r\n    }\r\n .questions  .content-right .detail-list #addQuestion3 {\r\n        min-height: 100%;\r\n      }\r\n .questions  .content-right .el-table {\r\n      height: 100%;\r\n      min-height: 100%;\r\n      max-height: none;\r\n      \r\n      \r\n\r\n      \r\n    }\r\n .questions  .content-right .el-table ol li {\r\n        list-style: upper-alpha;\r\n        line-height: 30px;\r\n        padding: 0 7px;\r\n        margin: 0 20px;\r\n      }\r\n .questions  .content-right .el-table .demo-table-expand label {\r\n        width: 65px;\r\n        color: #99a9bf;\r\n      }\r\n.questions  .content-right .el-table .demo-table-expand {\r\n        font-size: 0;\r\n        padding: 0 99px;\r\n        padding-left: 50px;\r\n        \r\n        \r\n        \r\n      }\r\n.questions  .content-right .el-table .demo-table-expand .el-form-item {\r\n          margin-right: 0;\r\n          margin-bottom: 0;\r\n          padding-left: 20px;\r\n          width: 50%;\r\n        }\r\n\r\n.questions  .content-right .el-table .demo-table-expand .el-form-item:first-child {\r\n          float: left;\r\n          height: 100%;\r\n        }\r\n.questions  .content-right .el-table .demo-table-expand .el-form-item:first-child {\r\n          max-width: 70%;\r\n        }\r\n.questions  .content-right .el-table .el-table__body-wrapper {\r\n        min-height: 290px;\r\n      }\r\n</style>\r\n"]}]}